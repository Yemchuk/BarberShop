<table class="table table-striped">	
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">Name</th>
			<th scope="col">Phone</th>
			<th scope="col">Date</th>
			<th scope="col">Barber</th>
			<th scope="col">Color</th>
		</tr>
	</thead>
	<% @results.each do |row|%>
		<tbody>
			<tr>
				<th><%= row['Id']%></th>
				<td><%= row['username']%></td>
				<td><%= row['Phone']%></td>
				<td><%= row['DateStamp']%></td>
				<td><%= row['Barber']%></td>
				<td><%= row['Color']%></td>
			</tr>
		</tbody>		
	<% end %>
</table>
